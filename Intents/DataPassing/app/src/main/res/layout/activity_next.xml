<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NextActivity">

    <TextView
        android:id="@+id/userNameTextNext"
        android:layout_width="245dp"
        android:layout_height="60dp"
        android:layout_marginStart="83dp"
        android:layout_marginTop="81dp"
        android:layout_marginEnd="83dp"
        android:layout_marginBottom="11dp"
        android:gravity="center"
        android:text="Username: "
        app:layout_constraintBottom_toTopOf="@+id/emailTextNext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/emailTextNext"
        android:layout_width="245dp"
        android:layout_height="60dp"
        android:layout_marginStart="83dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="83dp"
        android:layout_marginBottom="391dp"
        android:gravity="center"
        android:text="EmailID:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userNameTextNext" />
</androidx.constraintlayout.widget.ConstraintLayout>